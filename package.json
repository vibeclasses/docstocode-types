{
  "name": "@vibeclasses/docstocode-types",
  "version": "1.3.2",
  "description": "Shared TypeScript types for DocsToCode",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "import": "./dist/schemas.js",
      "require": "./dist/schemas.cjs"
    },
    "./validators": {
      "types": "./dist/validators.d.ts",
      "import": "./dist/validators.js",
      "require": "./dist/validators.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "typescript",
    "types",
    "project-management",
    "shared"
  ],
  "author": "Marco Antonio Gonzalez Junior <m@rco.sh>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.29.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vibeclasses/docstocode-types.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "json-schema": "^0.4.0"
  }
}
